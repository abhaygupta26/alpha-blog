<h1>Articles Listing</h1>

<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Description</th>
			<th>Actions</th>
		</tr>
	</thead>

	<tbody>
		<% @articles.each do |article| %>
		<thead>
			<tr>
				<td><%= article.title %></td>
				<td><%= article.description %></td>
				<td></td>
			</tr>
		</thead>
		<% end %>
	</tbody>
</table>